package qOne;

public class ArmstongNumber {
	
	public boolean isArmstrong(int a){
		int real = a;
		int sumq = 0;
		while(real >0) {
			sumq = sumq+((real%10)*(real%10)*(real%10));
			real = real/10;
		}
		if(sumq != a) return false;
		
		return true;
		
		
	}
	
	
	public static void main(String [] args) {
		
	}

}
